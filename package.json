{
  "name": "selby.dev",
  "version": "0.0.1",
  "scripts": {
    "test": "jest",
    "dev": "nodemon #next",
    "build": "next build && tsc --project tsconfig.server.json #next build",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js #next start",
    "format": "prettier-eslint --list-different --ignore-path .gitignore  --write '**/*.{js,jsx,json,ts,tsx}' '.storybook/**/*.{js,jsx,json,ts,tsx}'",
    "commit": "npx git-cz",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "bulma": "^0.7.5",
    "morgan": "^1.9.1",
    "next": "latest",
    "node-sass": "^4.12.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.11",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack-env": "^1.14.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.4",
    "jest": "^24.5.0",
    "lint-staged": "^9.2.3",
    "nodemon": "^1.19.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^1.11.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.4",
    "standard-version": "^7.0.0",
    "storybook-addon-specifications": "^2.1.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "git add"
    ]
  }
}
